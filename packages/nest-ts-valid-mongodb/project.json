{
  "name": "nest-ts-valid-mongodb",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nest-ts-valid-mongodb/src",
  "projectType": "library",
  "release": {
    "version": {
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nest-ts-valid-mongodb",
        "tsConfig": "packages/nest-ts-valid-mongodb/tsconfig.lib.json",
        "project": "packages/nest-ts-valid-mongodb/package.json",
        "entryFile": "packages/nest-ts-valid-mongodb/src/index.ts",
        "external": ["ts-valid-mongodb"],
        "rollupConfig": "packages/nest-ts-valid-mongodb/rollup.config.cjs",
        "compiler": "swc",
        "format": ["cjs", "esm"],
        "assets": [
          {
            "glob": "*.md",
            "input": "packages/nest-ts-valid-mongodb",
            "output": "."
          },
          {
            "glob": "LICENSE",
            "input": "packages/nest-ts-valid-mongodb",
            "output": "."
          },
          {
            "glob": "package.json",
            "input": "packages/nest-ts-valid-mongodb",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "reportsDirectory": "../../coverage/packages/nest-ts-valid-mongodb"
      }
    }
  }
}
